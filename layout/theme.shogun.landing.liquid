  {{ content_for_layout }}
  
  <script type="text/javascript" src='https://crm.zoho.com/crm/javascript/zcga.js'> </script>
  <style type="text/css">
    body, html {
      width: 100% !important;
      background: white !important;
    }
  </style>
  
</body>

